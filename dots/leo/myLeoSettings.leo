# remove trailing whitespaces for every line in the current node's body
# command is bound to shortcut in the @keys section

CommandName = 'rstrip-node'

body = c.frame.body
cursor = body.widget.textCursor()
cp = cursor.position()  # remember position - cause updateBodyPane changes it

# FIXME: both algos are buggy! cursor is set to the prev pos aftewards but!
# since some spaces can be removed above the cursor - it will jump forward
# for the amount of removed spaces

if True:  # straightforward version
    b = c.undoer.beforeChangeNodeContents(p)
    content = "\n".join( (l.rstrip() for l in c.p.b.splitlines()) )
    c.p.b = content
    c.undoer.afterChangeNodeContents(p, command=CommandName, bunch=b)

else:  # sophisticated and cryptic version
    # rstrip all lines
    head, lines, tail, oldSel, oldYview = c.getBodyLines()  # lines above, under and below cursor
    rstrip_lines = lambda ll: "\n".join( (l.rstrip() for l in ll) )
    head = rstrip_lines(head.splitlines())
    lines = rstrip_lines(lines)
    tail = rstrip_lines(tail.splitlines())

    # set rstripped lines
    prefix, suffix = '\n' if head else '', '\n' if tail else ''
    c.updateBodyPane(head, prefix+lines+suffix, tail,
                     undoType=CommandName, oldSel=None, oldYview=oldYview)


body.wrapper.setSelectionRange(cp, cp)  # drop any selection and return cursor to where it was</t>
