def __acquire_module(name, forceReload):
    import sys
    import os.path
    import importlib.util
    if (not forceReload and name in sys.modules):
        module = sys.modules[name]
    else:
        configLibPath = os.path.expanduser(c.config.getString('confg-lib-dir'))
        rootDir = configLibPath + "/"
        leostuffLibsDir = rootDir + name + ".py"
        spec = importlib.util.spec_from_file_location(name, leostuffLibsDir)
        module = importlib.util.module_from_spec(spec)
        sys.modules[spec.name] = module
        spec.loader.exec_module(module)
    return module


def import_leo_user_libs(*names, forceReload=False):
    try:
        iter(names)
    except TypeError:
        names = [names]
    for name in names:
        module = __acquire_module(name, forceReload)
        list(map(lambda k: setattr(module, k, globals().get(k, None)),
                 filter(lambda k: hasattr(module, k), ["c", "g"])))
        # [setattr(module, k, globals().get(k, None)) for k in ["c", "g"] if hasattr(module, k)]
