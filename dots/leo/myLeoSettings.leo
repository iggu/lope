@raw


body = """@language html

&lt;html&gt;
    &lt;head&gt;
        &lt;meta charset="utf-8"&gt;
        &lt;title&gt;&lt;/title&gt;

        &lt;style&gt;
            &lt;&lt; style &gt;&gt;
        &lt;/style&gt;

        &lt;script&gt;
            &lt;&lt; script :: head &gt;&gt;
        &lt;/script&gt;
    &lt;/head&gt;

    &lt;body&gt;
        &lt;&lt; body &gt;&gt;

        &lt;script&gt;
            &lt;&lt; script :: body &gt;&gt;
        &lt;/script&gt;
    &lt;/body&gt;
&lt;/html&gt;
"""

undoer = c.undoer
undoType = 'Create new HTML @clean file'
undoer.beforeChangeGroup(c.p, undoType)  # 1st arg - position to select after undo
bbbb = undoer.beforeInsertNode(c.p)

pRoot = c.p.insertAsNthChild(0)
pRoot.h = f'@clean .html'
pRoot.expand()

pScriptH = pRoot.insertAsLastChild()
pScriptH.h = "&lt;&lt; script :: head &gt;&gt;"
pScriptH.b = "@language javascript\n\n"

pScriptB = pRoot.insertAsLastChild()
pScriptB.h = "&lt;&lt; script :: body &gt;&gt;"
pScriptB.b = "@language javascript\n\n"

pStyle = pRoot.insertAsLastChild()
pStyle.h = "&lt;&lt; style &gt;&gt;"
pStyle.b = "@language css\n\n"

pHtml = pRoot.insertAsLastChild()
pHtml.h = "&lt;&lt; body &gt;&gt;"
pHtml.b = "@language html\n\n"

pRoot.b = body

c.setChanged(True)
c.selectPosition(pRoot)
c.redraw()

k = c.k
k.setLabelBlue('Filename: ')

def pRoot_set_filename(_):
    pRoot.h = f'@clean {k.arg}.html'
    k.clearState()
    k.resetLabel()
    k.showStateAndMode()
    undoer.afterInsertNode(pRoot, 'insert', bbbb)
    c.treeWantsFocus()

k.get1Arg(None, handler=pRoot_set_filename)
